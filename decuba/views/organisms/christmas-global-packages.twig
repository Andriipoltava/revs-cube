// Festive.
		$('#festive-select').on('change', function () {
			var selectedBar = this.value;

			if (selectedBar == 'aberdeen' || selectedBar == 'belfast' || selectedBar == 'glasgow' || selectedBar == 'birmingham' || selectedBar == 'bristol' || selectedBar == 'liverpool' || selectedBar == 'milton_keynes' || selectedBar == 'reading' || selectedBar == 'southampton' || selectedBar == 'cardiff' || selectedBar == 'derby' || selectedBar == 'harrogate' || selectedBar == 'huddersfield' || selectedBar == 'newcastle' || selectedBar == 'sheffield') { // LOW.
