<script>
	$(document).ready(function () {
		$(".view-menu").click(function () {
			var menu = $(this).parent().parent().parent().parent().parent().find(".packages-item-menu");
			menu.addClass("show");
			menu.parent().addClass("opened-section");

		})

		$(".close-menu").click(function () {
			var menu = $(this).parent().parent();
			menu.parent().removeClass("opened-section");
			menu.removeClass("show");
		})

// ----------------Tabs-----------------

		$('.tab-label').click(function () {
			var tab_id = $(this).attr('data-tab');
			console.log(tab_id);
			$(this).parent().find(".tab-label").removeClass('active');
			$(this).addClass('active');
			$(this).parent().parent().parent().find(".menu-list").removeClass("active");
			$(this).parent().parent().parent().find("." + tab_id).addClass('active');
		})

// ----------------Update price with bar selector---------------
// 3 course.
		$('#3-course-select').on('change', function () {
			var selectedBar = this.value;

			if (selectedBar == 'cardiff' || selectedBar == 'derby' || selectedBar == 'glasgow' || selectedBar == 'harrogate' || selectedBar == 'huddersfield' || selectedBar == 'newcastle' || selectedBar == 'norwich' || selectedBar == 'sheffield') { // LOW.
				$('#3-course-price').text('£20.95 pp');
			} else if (selectedBar == 'aberdeen' || selectedBar == 'bristol' || selectedBar == 'leeds' || selectedBar == 'liverpool' || selectedBar == 'milton_keynes' || selectedBar == 'nottingham' || selectedBar == 'reading' || selectedBar == 'southampton') { // MEDIUM.
				$('#3-course-price').text('£23.95 pp');
			} else if (selectedBar == 'belfast' || selectedBar == 'birmingham' || selectedBar == 'manchester') { // HIGH.
				$('#3-course-price').text('£25.95 pp');
			}
		});

// Tapas.
		$('#tapas-select').on('change', function () {
			var selectedBar = this.value;

			if (selectedBar == 'cardiff' || selectedBar == 'derby' || selectedBar == 'glasgow' || selectedBar == 'harrogate' || selectedBar == 'huddersfield' || selectedBar == 'newcastle' || selectedBar == 'norwich' || selectedBar == 'sheffield') { // LOW.
